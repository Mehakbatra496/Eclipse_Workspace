package com.example2.dao;
import java.util.*;
import com.example2.dao.RegisterDao;
import com.example2.beans.*;
import org.springframework.jdbc.core.JdbcTemplate;
import org.springframework.stereotype.Repository;
import org.springframework.beans.factory.annotation.Autowired;

@Repository
public class RegisterDaoImpl implements RegisterDao
{
	@Autowired
	JdbcTemplate jdbcTemplate;
	
	@Override
	public boolean registerDao(Student std)
	{
	
		boolean bb= false;
		String sql= "insert into register2 values(?,?,?,?,?)";
		int count=JdbcTemplate.update(sql, new Object[] {std.getName(),std.getEmail(), std.getPassword(), std.getCity(), std.getGender()});
		
		
		
		return bb;
	}


}
