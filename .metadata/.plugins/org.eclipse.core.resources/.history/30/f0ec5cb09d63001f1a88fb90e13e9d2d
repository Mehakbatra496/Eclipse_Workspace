package com.example2.services;
import com.example2.beans.Student;
import com.example2.dao.LoginDao;
import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

@Service 
public interface LoginServiceImpl implements LoginService 
{
	@Autowired
	LoginDao loginDao;
	
	@Override

	public List<Student> loginService(String email, String password)
	{
	
	List<Student> students_list =loginDao.loginDao(String email, String password);
	
	return students_list;
	}
}
